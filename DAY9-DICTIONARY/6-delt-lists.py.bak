sample_dict = {
    "name": "Kelly",
    "age": 25,
    "salary": 8000,
    "city": "New york"}
keys = ["name", "salary"] 
my = {}
print()
for j in sample_dict:
        if j not in keys:
            my.update({j : sample_dict[j]})
print(my)

